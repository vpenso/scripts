{
    "layer": "top",
    "height": 20,
    "margin": 5,
    "modules-left": ["custom/clock","sway/workspaces"],
    "modules-right": ["battery"],
    "battery": {
        "format": "{capacity}% {time}",
    },
    "custom/clock": {
        "exec": "date +'%Y/%m/%d %H:%M'",
        "interval": 5
    },
}
