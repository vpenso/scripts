{
    "layer": "top",
    "height": 20,
    "margin": 5,
    "modules-left": ["custom/clock"],
    "modules-center": ["sway/workspaces"],
    "modules-right": ["battery"],
    "battery": {
        "format": "{capacity}% {icon}",
        "format-icons": ["", "", "", "", ""]
    },
    "custom/clock": {
        "exec": "date +'%Y/%m/%d %H:%M'",
        "interval": 5
    },
}
